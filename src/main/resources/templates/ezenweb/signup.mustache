{{>ezenweb/header}}
    <h3> 회원가입 페이지 </h3>
    <form class="signUpForm">


        <div> 아이디 </div>
            <input onkeyup="idcheck()" type="text" id="id" name="id" />
        <div class="idcheckbox">유효성검사결과</div>

        <div> 비밀번호 </div>
        <input onkeyup="pwcheck()"  type="text" id="pw" name="pw" />
        
        <div> 비밀번호 확인 </div>
        <input onkeyup="pwcheck()"  type="text" id="pwconfirm" name="pwconfirm"/>
        <div class="pwcheckbox">유효성검사결과</div>

        이름 : <input type="text" id="name" name="name"/> <br/>
        전화번호 : <input onkeyup="phonecheck()" type="text" id="phone" name="phone" /> <br/>
        이메일 : <input type="text" id="email" name="email" /> <br/>

        프로필 사진 :
        <input onchange="onChangeImg( this )" type="file" id="img"
               name="img" accept="image/*" /> <br/>

        미리보기 :
        <div>    <img id="preimg" src="/img/default.jpg" />   </div>

        <button type="button" onclick="signup(  )">회원가입</button>
        <a href="/member/login">로그인</a>


    </form>
    <script src="/js/member.js"></script>   <!-- {resources/static} /js/member.js -->
{{>ezenweb/footer}}